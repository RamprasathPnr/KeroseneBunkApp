<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <include
        android:id="@+id/topLayout"
        layout="@layout/layout_top_layout_other"
        android:layout_width="fill_parent"
        android:layout_height="90dp"
        android:layout_alignParentTop="true" />

    <LinearLayout
        android:id="@+id/parentButLayout"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_above="@+id/bottom_layout"
        android:layout_below="@+id/topLayout"
        android:layout_margin="30dp"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/firstButLayout"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="horizontal">

            <RelativeLayout
                android:id="@+id/stock_inward_lay"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <RelativeLayout
                    android:id="@+id/stock_inward"
                    android:layout_width="fill_parent"
                    android:layout_height="90dp"
                    android:layout_centerInParent="true"
                    android:layout_margin="30dp"
                    android:background="@drawable/all_grid_back"
                    android:padding="2dp">

                    <com.omneAgate.Util.SquareLayout
                        android:id="@+id/qrImage"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:background="#029451">

                        <ImageView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:src="@drawable/icon_inward" />
                    </com.omneAgate.Util.SquareLayout>

                    <TextView
                        android:id="@+id/stock_inward_tv"
                        android:layout_width="fill_parent"
                        android:layout_height="90dp"
                        android:layout_toRightOf="@+id/qrImage"
                        android:gravity="center"
                        android:text="Stock Inward"
                        android:textColor="#424141"
                        android:textSize="25dp" />
                </RelativeLayout>

                <TextView
                    android:id="@+id/textQRDescription"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_margin="15dp"
                    android:textColor="#626262"
                    android:textSize="12dp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/stock_check_lay"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <RelativeLayout
                    android:id="@+id/stock_check"
                    android:layout_width="fill_parent"
                    android:layout_height="90dp"
                    android:layout_centerInParent="true"
                    android:layout_margin="30dp"
                    android:background="@drawable/all_grid_back"
                    android:padding="2dp">

                    <com.omneAgate.Util.SquareLayout
                        android:id="@+id/mobileImage"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:background="#0A40AB">

                        <ImageView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:src="@drawable/icon_stock_check" />
                    </com.omneAgate.Util.SquareLayout>

                    <TextView
                        android:id="@+id/stock_status_tv"
                        android:layout_width="fill_parent"
                        android:layout_height="90dp"
                        android:layout_toRightOf="@+id/mobileImage"
                        android:gravity="center"
                        android:text="Stock Status"
                        android:textColor="#424141"
                        android:textSize="25dp" />
                </RelativeLayout>

                <TextView
                    android:id="@+id/textMobileDescription"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_margin="10dp"
                    android:textColor="#626262"
                    android:textSize="12dp" />
            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/secondButLayout"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="horizontal">

            <RelativeLayout
                android:id="@+id/correct_history_lay"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <RelativeLayout
                    android:id="@+id/correct_history"
                    android:layout_width="fill_parent"
                    android:layout_height="90dp"
                    android:layout_centerInParent="true"
                    android:layout_margin="30dp"
                    android:background="@drawable/all_grid_back"
                    android:padding="2dp">

                    <com.omneAgate.Util.SquareLayout
                        android:id="@+id/historyImage"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:background="#029451">
                        <ImageView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:src="@drawable/icon_othermenu" />
                    </com.omneAgate.Util.SquareLayout>

                    <TextView
                        android:id="@+id/correct_history_tv"
                        android:layout_width="fill_parent"
                        android:layout_height="90dp"
                        android:layout_toRightOf="@+id/historyImage"
                        android:gravity="center"
                        android:text="Correction History"
                        android:textColor="#424141"
                        android:textSize="25dp" />
                </RelativeLayout>

                <TextView
                    android:id="@+id/textHistoryDescription"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_margin="15dp"
                    android:textColor="#626262"
                    android:textSize="12dp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/dummy_lay_one"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <RelativeLayout
                    android:id="@+id/dummy_lay_two"
                    android:layout_width="fill_parent"
                    android:layout_height="90dp"
                    android:layout_centerInParent="true"
                    android:layout_margin="30dp"
                    android:padding="2dp">

                    <com.omneAgate.Util.SquareLayout
                        android:id="@+id/dummyImage"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent">

                        <ImageView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"/>
                    </com.omneAgate.Util.SquareLayout>

                    <TextView
                        android:id="@+id/dummy_tv"
                        android:layout_width="fill_parent"
                        android:layout_height="90dp"
                        android:layout_toRightOf="@+id/dummyImage"
                        android:gravity="center"
                        android:text=""
                        android:textColor="#424141"
                        android:textSize="25dp" />
                </RelativeLayout>

                <TextView
                    android:id="@+id/textDummyDescription"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_margin="10dp"
                    android:textColor="#626262"
                    android:textSize="12dp" />
            </RelativeLayout>

        </LinearLayout>

    </LinearLayout>

    <include
        android:id="@+id/bottom_layout"
        layout="@layout/user_bottom_other_screen"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true" />

</RelativeLayout>
